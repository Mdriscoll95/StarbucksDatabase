@model IEnumerable<Stores>

<h2class="colorChange">Stores</h2class="colorChange">
<table class="tableColor">
    <div style="display: block">
        <a href="/Stores/InsertStores/">Create a New Store</a>
<tr>
        <th>ID</th>
        <th>Location</th>
        <th>Managers</th>
        <th>NumberOfEmployees</th>
    </tr>

    @foreach (var stores in @Model)
    {
        <tr>
            <td><a href=/Stores/ViewStores/@stores.StoreID>@stores.StoreID</a></td>
            <td>@stores.Location</td>
            <td>@stores.Manager</td>
            <td>@stores.NumberOfEmployees</td>

        </tr>
    }

</table>
<div style="display: block">
    <a href="/Stores/InsertStores/">Create a New Store</a>
</div>


</table>
<video id="video-bg" autoplay muted loop>
    <source src="@Url.Content("~/Media/Stores.mp4")" type="video/mp4">
    Your browser does not support the video tag.
</video>